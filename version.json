{
  "$schema": "https://raw.githubusercontent.com/AArnott/Nerdbank.GitVersioning/master/src/NerdBank.GitVersioning/version.schema.json",
  "version": "1.3.7",
  "publicReleaseRefSpec": [
    //"^refs/heads/main$", 
    "^refs/tags/V\\d+\\.\\d+" // we also release tags starting with vN.N
    "^refs/tags/v\\d+\\.\\d+"
    "^refs/tags/V\\d+\\.\\d+\\.\\d+"
    "^refs/tags/v\\d+\\.\\d+\\.\\d+"
    "^refs/tags/d+\\.\\d+\\.\\d+"
    "^refs/tags/d+\\.\\d+"
    "^refs/tags/-alpha"
  ],
  "cloudBuild": {
    "buildNumber": {
      "enabled": false
    }
  }
}
